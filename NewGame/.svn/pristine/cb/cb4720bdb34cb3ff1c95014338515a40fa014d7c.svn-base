@layout("/common/_container.html"){
	<!-- PAGE CONTENT BEGINS -->
<div class="row">
	<div class="col-xs-12 tb-grid">
	<form data-url="${ctxPath}/${code}/update" method="post" class="form-horizontal" role="form">
		<table class="table table-bordered">
			<tbody>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">包ID: </td>
					<td colspan="3">${accounttypename.clienttype!}
						<input name="accounttypename.clientType" value="${accounttypename.clienttype!}" type="hidden"/>
					</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">包名: </td>
					<td>
						<input name="accounttypename.name" value="${accounttypename.name!}"/>
					</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">副标题: </td>
					<td><input name="accounttypename.subName" value="${accounttypename.subname!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">热词: </td>
					<td colspan="3"><textarea rows="6" cols="80" name="accounttypename.hotword">${accounttypename.hotword!}</textarea></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">皮类型: </td>
					<td><input name="accounttypename.ABCD" value="${accounttypename.abcd!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">链接: </td>
					<td><input name="accounttypename.link" value="${accounttypename.link!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">分类1: </td>
					<td><input name="accounttypename.lay1" value="${accounttypename.lay1!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">分类2: </td>
					<td><input name="accounttypename.lay2" value="${accounttypename.lay2!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">刷榜次数: </td>
					<td><input name="accounttypename.rankTimes" value="${accounttypename.ranktimes!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">热词刷新次数: </td>
					<td><input name="accounttypename.hotTimes" value="${accounttypename.hottimes!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">刷过的热词：排名: </td>
					<td colspan="3"><textarea rows="6" cols="80" name="accounttypename.moneyhotwords">${accounttypename.moneyhotwords!}</textarea></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">榜单状态: </td>
					<td colspan="3">
						@if(accounttypename.rankstaus==0){
							<input type="radio" name="accounttypename.rankStaus" value="0" checked="checked"/>进入榜单
							<input type="radio" name="accounttypename.rankStaus" value="1"/>移除榜单
							<input type="radio" name="accounttypename.rankStaus" value="2"/>下架
							<input type="radio" name="accounttypename.rankStaus" value="3"/>未上架
						@}else if(accounttypename.rankstaus==1){
							<input type="radio" name="accounttypename.rankStaus" value="0"/>进入榜单
							<input type="radio" name="accounttypename.rankStaus" value="1" checked="checked"/>移除榜单
							<input type="radio" name="accounttypename.rankStaus" value="2"/>下架
							<input type="radio" name="accounttypename.rankStaus" value="3"/>未上架
						@}else if(accounttypename.rankstaus==2){
							<input type="radio" name="accounttypename.rankStaus" value="0"/>进入榜单
							<input type="radio" name="accounttypename.rankStaus" value="1"/>移除榜单
							<input type="radio" name="accounttypename.rankStaus" value="2" checked="checked"/>下架
							<input type="radio" name="accounttypename.rankStaus" value="3"/>未上架
						@}else{
							<input type="radio" name="accounttypename.rankStaus" value="0"/>进入榜单
							<input type="radio" name="accounttypename.rankStaus" value="1"/>移除榜单
							<input type="radio" name="accounttypename.rankStaus" value="2"/>下架
							<input type="radio" name="accounttypename.rankStaus" value="3" checked="checked"/>未上架
						@}
					</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">审核状态: </td>
					<td colspan="3">
						@if(accounttypename.checkstaus==0){
							<input type="radio" name="accounttypename.checkStaus" value="0" checked="checked"/>准备上传
							<input type="radio" name="accounttypename.checkStaus" value="1"/>上传
							<input type="radio" name="accounttypename.checkStaus" value="2"/>等待审核
							<input type="radio" name="accounttypename.checkStaus" value="3"/>正在审核
							<input type="radio" name="accounttypename.checkStaus" value="4"/>退回
							<input type="radio" name="accounttypename.checkStaus" value="5"/>审核通过
							<input type="radio" name="accounttypename.checkStaus" value="6"/>更新包
						@}else if(accounttypename.checkstaus==1){
							<input type="radio" name="accounttypename.checkStaus" value="0"/>准备上传
							<input type="radio" name="accounttypename.checkStaus" value="1" checked="checked"/>上传
							<input type="radio" name="accounttypename.checkStaus" value="2"/>等待审核
							<input type="radio" name="accounttypename.checkStaus" value="3"/>正在审核
							<input type="radio" name="accounttypename.checkStaus" value="4"/>退回
							<input type="radio" name="accounttypename.checkStaus" value="5"/>审核通过
							<input type="radio" name="accounttypename.checkStaus" value="6"/>更新包
						@}else if(accounttypename.checkstaus==2){
							<input type="radio" name="accounttypename.checkStaus" value="0"/>准备上传
							<input type="radio" name="accounttypename.checkStaus" value="1"/>上传
							<input type="radio" name="accounttypename.checkStaus" value="2" checked="checked"/>等待审核
							<input type="radio" name="accounttypename.checkStaus" value="3"/>正在审核
							<input type="radio" name="accounttypename.checkStaus" value="4"/>退回
							<input type="radio" name="accounttypename.checkStaus" value="5"/>审核通过
							<input type="radio" name="accounttypename.checkStaus" value="6"/>更新包
						@}else if(accounttypename.checkstaus==3){
							<input type="radio" name="accounttypename.checkStaus" value="0"/>准备上传
							<input type="radio" name="accounttypename.checkStaus" value="1"/>上传
							<input type="radio" name="accounttypename.checkStaus" value="2"/>等待审核
							<input type="radio" name="accounttypename.checkStaus" value="3" checked="checked"/>正在审核
							<input type="radio" name="accounttypename.checkStaus" value="4"/>退回
							<input type="radio" name="accounttypename.checkStaus" value="5"/>审核通过
							<input type="radio" name="accounttypename.checkStaus" value="6"/>更新包
						@}else if(accounttypename.checkstaus==4){
							<input type="radio" name="accounttypename.checkStaus" value="0"/>准备上传
							<input type="radio" name="accounttypename.checkStaus" value="1"/>上传
							<input type="radio" name="accounttypename.checkStaus" value="2"/>等待审核
							<input type="radio" name="accounttypename.checkStaus" value="3"/>正在审核
							<input type="radio" name="accounttypename.checkStaus" value="4" checked="checked"/>退回
							<input type="radio" name="accounttypename.checkStaus" value="5"/>审核通过
							<input type="radio" name="accounttypename.checkStaus" value="6"/>更新包
						@}else if(accounttypename.checkstaus==5){
							<input type="radio" name="accounttypename.checkStaus" value="0"/>准备上传
							<input type="radio" name="accounttypename.checkStaus" value="1"/>上传
							<input type="radio" name="accounttypename.checkStaus" value="2"/>等待审核
							<input type="radio" name="accounttypename.checkStaus" value="3"/>正在审核
							<input type="radio" name="accounttypename.checkStaus" value="4"/>退回
							<input type="radio" name="accounttypename.checkStaus" value="5" checked="checked"/>审核通过
							<input type="radio" name="accounttypename.checkStaus" value="6"/>更新包
						@}else{
							<input type="radio" name="accounttypename.checkStaus" value="0"/>准备上传
							<input type="radio" name="accounttypename.checkStaus" value="1"/>上传
							<input type="radio" name="accounttypename.checkStaus" value="2"/>等待审核
							<input type="radio" name="accounttypename.checkStaus" value="3"/>正在审核
							<input type="radio" name="accounttypename.checkStaus" value="4"/>退回
							<input type="radio" name="accounttypename.checkStaus" value="5"/>审核通过
							<input type="radio" name="accounttypename.checkStaus" value="6" checked="checked"/>更新包
						@}
					</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">上架时间: </td>
					<td><input id="okTime" name="accounttypename.okTime" value="${accounttypename.oktime!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">备注: </td>
					<td><input name="accounttypename.desc" value="${accounttypename.desc!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">刷榜效果评级: </td>
					<td><input name="accounttypename.rankResultLevel" value="${accounttypename.rankresultlevel!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">更新包次数: </td>
					<td><input name="accounttypename.updateTimes" value="${accounttypename.updatetimes!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">aso地址: </td>
					<td><input name="accounttypename.asoLink" value="${accounttypename.asolink!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">是否是白名单开启: </td>
					<td>
						@if(accounttypename.iswhitelistenabled==1){
							<input type="radio" name="accounttypename.IsWhiteListEnabled" value="1" checked="checked"/>是
							<input type="radio" name="accounttypename.IsWhiteListEnabled" value="0"/>否
						@}else{
							<input type="radio" name="accounttypename.IsWhiteListEnabled" value="1"/>是
							<input type="radio" name="accounttypename.IsWhiteListEnabled" value="0" checked="checked"/>否
						@}
					</td>
				</tr>
			</tbody>
		</table>
		</form>
	</div>
</div>
<script src="${ctxPath}/static/laydate/laydate.js"></script>
<script type="text/javascript">
	laydate({
		elem : '#okTime',
		format : 'YYYY-MM-DD hh:mm:ss', // 分隔符可以任意定义，该例子表示只显示年月日
		istime: true,
		choose : function(datas) { //选择日期完毕的回调
			/*   alert('得到：'+datas); */
		}
	});
</script>
		<!-- 按钮 -->
		@ include("/common/_curd/_btn.html",{btn_save:"确认修改",btn_close:"关闭"}) {}
@}