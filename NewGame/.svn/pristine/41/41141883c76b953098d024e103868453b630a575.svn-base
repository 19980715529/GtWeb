package com.smallchill.db.config.model;

public enum ConfigInfoInit {
	
	CI_ContactConfig("ContactConfig", "联系方式配置"),
	
	CI_SiteConfig("SiteConfig", "站点配置"),
	
	CI_GameFullPackageConfig("GameFullPackageConfig", "大厅整包配置"),
	
	CI_GameJanePackageConfig("GameJanePackageConfig", "大厅简包配置"),
	
	CI_EmailConfig("EmailConfig", "邮箱配置"),
	
	CI_GameAndroidConfig("GameAndroidConfig", "安卓大厅配置"),
	
	CI_GameIosConfig("GameIosConfig", "苹果大厅配置"),
	
	CI_MobilePlatformVersion("MobilePlatformVersion", "移动版大厅配置")
	;

	// others

	private final String code;

	private final String description;

	private ConfigInfoInit(String code, String description) {
		this.code = code;
		this.description = description;
	}

	public String code() {
		return this.code;
	}

	public String getDescription() {
		return this.description;
	}
}
