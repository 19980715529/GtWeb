@layout("/common/_container.html"){
	<!-- PAGE CONTENT BEGINS -->
<div class="row">
	<div class="col-xs-12 tb-grid">
	<form data-url="${ctxPath}/${code}/update" method="post" class="form-horizontal" role="form">
		<table class="table table-bordered">
			<tbody>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏ID: </td>
					<td>${gamemobilekind.KindID!}
						<input type="hidden" name="gamemobilekind.KindID" value="${gamemobilekind.KindID!}"/>
					</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏名称: </td>
					<td><input type="text" name="gamemobilekind.KindName" value="${gamemobilekind.KindName!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">模板名称: </td>
					<td><input type="text" name="gamemobilekind.ModuleName"  value="${gamemobilekind.ModuleName!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">模板版本号: </td>
					<td><input type="text" name="gamemobilekind.ClientVersion"  value="${gamemobilekind.ClientVersion!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">资源版本号: </td>
					<td><input type="text" name="gamemobilekind.ResVersion"  value="${gamemobilekind.ResVersion!}"/></td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">排序: </td>
					<td><input type="text" name="gamemobilekind.SortID"  value="${gamemobilekind.SortID!}"/></td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏归属: </td>
					<td>
						@if((gamemobilekind.KindMark == 1) || (gamemobilekind.KindMark == 3)){
						<input type="checkbox" name="gamemobilekind.KindMark" value="1" checked="checked"/>IOS
						@} else {
						<input type="checkbox" name="gamemobilekind.KindMark" value="1"/>IOS
						@}
						@if((gamemobilekind.KindMark == 2) || (gamemobilekind.KindMark == 3)){
						<input type="checkbox" name="gamemobilekind.KindMark" value="2" checked="checked"/>Android
						@} else {
						<input type="checkbox" name="gamemobilekind.KindMark" value="2"/>Android
						@}
					</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">禁用状态: </td>
					<td>
						@if(gamemobilekind.Nullity == 0){
						<input type="radio" name="gamemobilekind.Nullity" value="0" checked="checked"/>启用
						<input type="radio" name="gamemobilekind.Nullity" value="1"/>禁用
						@} else if(gamemobilekind.Nullity == 1){
						<input type="radio" name="gamemobilekind.Nullity" value="0"/>启用
						<input type="radio" name="gamemobilekind.Nullity" value="1" checked="checked"/>禁用
						@}
					</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏类型: </td>
					<td colspan="3">${gamemobilekind.TypeName!}</td>
				</tr>
			</tbody>
		</table>
		</form>
	</div>
</div>
		<!-- 按钮 -->
		@ include("/common/_curd/_btn.html",{btn_save:"确认修改",btn_close:"关闭"}) {}
@}