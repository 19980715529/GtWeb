@layout("/common/_container.html"){
<!-- PAGE CONTENT BEGINS -->
<div class="row">
    <div class="col-xs-12 tb-grid">
        <form data-url="${ctxPath}/${code}/save" method="post" class="form-horizontal" role="form">
            <table class="table table-bordered">
                <tbody>
                <tr>
                    <td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">订单编号: </td>
                    <td>
                        <input name="WithdrawalRequest.orderID" rows="" cols="" placeholder="订单编号" value="${id!}"></input>
                    </td>
                    <td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">确认用户ID: </td>
                    <td><input name="WithdrawalRequest.UserID" placeholder="用户ID" value="${Userid!}"/></td>

                </tr>
                </tbody>
            </table>
        </form>
    </div>
</div>
<!-- 按钮 -->
@ include("/common/_curd/_btn.html",{btn_save:"确认已提现",btn_close:"关闭"}) {}
@}