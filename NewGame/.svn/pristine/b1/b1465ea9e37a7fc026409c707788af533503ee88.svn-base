@layout("/common/_container.html"){
<!-- PAGE CONTENT BEGINS -->
<div class="row">
    <div class="col-xs-12 tb-grid">
        <form data-url="${ctxPath}/${code}/save" method="post" class="form-horizontal" role="form">
            <table class="table table-bordered">
                <tbody>
                <tr>
                    <td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">新增新手卡数量: </td>
                    <td>
                        <input name="serverGenerated.ExchangeNum" rows="" cols="" placeholder="新增数量"></input>
                    </td>
                    <td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">新增新手卡拥有着: </td>
                    <td>
                        <input name="serverGenerated.UserID" placeholder="商人ID"/>
                    </td>
                </tr>
                </tbody>
            </table>
        </form>
    </div>
</div>
<!-- 按钮 -->
@ include("/common/_curd/_btn.html",{btn_save:"确认新增",btn_close:"关闭"}) {}
@}