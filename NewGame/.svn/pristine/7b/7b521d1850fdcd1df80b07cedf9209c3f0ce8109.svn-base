@layout("/common/_container.html"){
	<!-- PAGE CONTENT BEGINS -->
<div class="row">
	<div class="col-xs-12 tb-grid">
		<table class="table table-bordered">
			<tbody>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏ID: </td>
					<td>${gamekind.KindID!}</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏名称: </td>
					<td>${gamekind.KindName!}</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏类型: </td>
					<td>${gamekind.TypeName!}</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">排序: </td>
					<td>${gamekind.SortID!}</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">进程名称: </td>
					<td>${gamekind.ProcessName!}</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">游戏属性: </td>
					<td>${gamekind.GameFlag!}</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">规则路径: </td>
					<td>${gamekind.GameRuleUrl!}</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">下载路径: </td>
					<td>${gamekind.DownLoadUrl!}</td>
				</tr>
				<tr>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">是否推荐: </td>
					<td>${gamekind.RecommendName!}</td>
					<td style="vertical-align: middle !important;text-align:right;" class="bg-F5F5F5">禁用状态: </td>
					<td>${gamekind.NullityName!}</td>
				</tr>
			</tbody>
		</table>
	</div>
</div>
		<!-- 按钮 -->
		@ include("/common/_curd/_btn.html",{btn_close:"关闭"}) {}
@}
